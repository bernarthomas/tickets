{
  "name": "tickets",
  "type": "project",
  "license": "GPL-3.0 license",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-pdo": "*",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.13",
    "ramsey/uuid": "^4.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Domaine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
